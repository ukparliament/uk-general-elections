<tr>
	<%= content_tag(  'td', link_to( candidacy.candidate_name_display, candidate_name_show_url( :candidate_name => candidacy.candidate_name_id ) ) ) %>
	<%= content_tag(  'td', link_to( candidacy.party_name, party_show_url( :party => candidacy.party_id )) ) %>
	<%= content_tag(  'td', candidacy.votes.to_s.reverse.gsub(/...(?=.)/,'\&,').reverse, :class => 'number' ) unless @election.is_uncontested %>
</tr>